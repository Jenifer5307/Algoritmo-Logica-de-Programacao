algoritmo "soma numerica"
var
   cont, s, n: inteiro
inicio
      escreval("Ol√°, bem-vindo ao sistema de soma numerica!")
      cont <- 1
      s <- 0
      enquanto (cont <= 5) faca
      escreva ("Digite o ",cont, "o. valor: ")
      leia (n)
      s <- s + n
      cont <- cont + 1
      FimEnquanto
      escreva ("A soma de todos os valores: ", s)
fimalgoritmo
